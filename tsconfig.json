{
  "compilerOptions": {
    "outDir": "lib/",
    "sourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "target": "es5",
    "declaration": true,
    "removeComments": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "pretty": true,
    "baseUrl": "./",
    "strictNullChecks": true
  }
}
