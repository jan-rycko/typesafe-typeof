    "microbundle": "^0.11.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3",
    "utility-types": "^3.7.0"
  },
  "dependencies": {
    "remedial": "^1.0.8"
  }
